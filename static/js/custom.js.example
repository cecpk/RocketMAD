       Name: "Grass Types Only",
       Searchstring: ['Grass'],
       Invert: false
     },
     {
       PokemonID: 29,
       Name: "Poison Types Only",
       Searchstring: ['Poison'],
       Invert: false
     },
     {
       PokemonID: 35,
       Name: "Fairy Types Only",
       Searchstring: ['Fairy'],
       Invert: false
     },
     {
       PokemonID: 364,
       Name: "Ice Types Only",
       Searchstring: ['Ice'],
       Invert: false
     },
     {
       PokemonID: 1,
       Name: "Generation 1 Only",
       Searchstring: ['gen1'],
